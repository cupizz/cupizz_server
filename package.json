{
  "name": "cubizz-server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:cubizz/cubizz-server.git",
  "author": "Hieren <hienlh1298@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn db:up && dotenv -- ts-node --transpile-only src/server.ts",
    "clean": "rm -rf dist",
    "build": "dotenv -- npm -s run clean && npm -s run generate && tsc",
    "generate": "dotenv -- prisma generate",
    "dev": "dotenv -- ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "db:save": "dotenv -- prisma migrate save --experimental",
    "db:up": "dotenv -- prisma migrate up --experimental",
    "db:down": "dotenv -- prisma migrate down 1 --experimental",
    "seed": "dotenv -- ts-node ./prisma/seed",
    "studio": "dotenv -- prisma studio"
  },
  "dependencies": {
    "@nexus/schema": "^0.18.0",
    "@types/node-fetch": "2.5.7",
    "@prisma/cli": "2.8.1",
    "@prisma/client": "2.8.1",
    "apollo-server-express": "^2.17.0",
    "bcryptjs": "^2.4.3",
    "cli-progress": "^3.8.2",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv-cli": "^3.2.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-upload": "11.0.0",
    "jsonwebtoken": "^8.5.1",
    "nexus-plugin-prisma": "^0.21.0",
    "nodemailer": "^6.4.11",
    "request": "^2.88.2",
    "subscriptions-transport-ws": "^0.9.18",
    "onesignal-node": "^3.2.0",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cli-progress": "^3.8.0",
    "@types/faker": "^5.1.2",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/graphql-upload": "8.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.6.4",
    "@types/nodemailer": "^6.4.0",
    "@types/request": "^2.48.5",
    "@types/uniqid": "^5.2.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "1.0.0-pre.50",
    "typescript": "^4.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
